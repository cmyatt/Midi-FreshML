# Clean existing compilation output
echo "Cleaning build/..."
rm _build/* 2> /dev/null;
echo "Cleaning bin/..."
rm bin/* 2> /dev/null;
echo "Removing generated lexer and parser..."
rm lexer.ml parser.ml parser.mli 2> /dev/null;
echo "Removing swap (~) files..."
rm *~ tests/*~ 2> /dev/null;

